
{% extends 'base.html.twig' %}
{% block title %}Weproject1: Mes Societes
{% endblock %}

    {% block header %}
        {% include "partials/_header.html.twig" %}
    {% endblock %}
    {% block body %}
        <div class="container">
            <h1>Mes Societes</1>
            <table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Numero</th>
      <th scope="col">Nom</th>
      <th scope="col">Address</th>
      <th scope="col">Description</th>
      <th scope="col">Date Creation</th>
      <th scope="col">Nombre Travailleur</th>

    </tr>
  </thead>
  <tbody>
        {% for societes in societe %}

            <tr class="table-primary">
            <th scope="row">{{societes.id}}</th>
            <td>{{societes.name}}</td>
            <td>{{societes.address}}</td>
            <td>{{societes.description}}</td>
            <td>{{ societes.createdAt|date('d/m/y') }}</td>
            </tr>
        {% endfor %}

  </tbody>
</table>
</div>
{% endblock %}
